<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.flea.mappers.MemberMapper">
	<insert id="join">
		insert into member (id,pw,name,nick)
		values(#{id},#{pw}, #{name}, #{nick})
	</insert>
	<select id="login" resultType="org.flea.domain.MemberVO">
	select ukey, nick from member where id=#{id} and pw=#{pw}

	</select>
</mapper>